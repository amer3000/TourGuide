<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.carrolltontourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RestaurantsActivity"
            android:label="Restaurants" />
        <activity
            android:name=".ParksActivity"
            android:label="Parks" />
        <activity
            android:name=".HistoricActivity"
            android:label="Historic Sites" />
        <activity
            android:name=".ShopsActivity"
            android:label="Shops" />
    </application>

</manifest>